if grep ' 6\.' /etc/redhat-release &>/dev/null; then
    _RELEASE="CENTOS6"
fi
if grep ' 7\.' /etc/redhat-release &>/dev/null; then
    _RELEASE="CENTOS7"
fi
if cat /etc/*-release|grep -i ubuntu &>/dev/null; then
    _RELEASE="UBUNTU"
fi

case $_RELEASE in
    CENTOS6)

	;;
    CENTOS7)

	;;
    UBUNTU)

	;;
esac
